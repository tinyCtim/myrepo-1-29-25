
echo
echo nzcom 10/21/17
echo
d tracks[0-7] 254
attach dsk0 nzcoma.dsk
attach dsk1 nzcomb.dsk
attach dsk2 nzcomc.dsk
attach dsk3 nzcomd.dsk
